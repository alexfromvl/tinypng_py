import os
import tinify

tiny_token = 'YOU TOKEN'

tinify.key = tiny_token
os.curdir
dir = os.path.abspath(os.curdir)
files = os.listdir(dir)
images = filter(lambda x: x.endswith('.png'), files)  #filter png files

for img in images:
	name = str(img)
	print (name)
	source = tinify.from_file(img)
	source.to_file(name)
